package com.learn.spring.jpa.entity;

import javax.persistence.Embeddable;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Embeddable
@Data
@AllArgsConstructor
@NoArgsConstructor
public class Guardian {

	private String name;
	private String email;
	private String mobile;
	
}
